{
  "name": "dev-backup-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "script"
  ],
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "dev:web": "pnpm --filter frontend dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter frontend build",
    "build:cli": "pnpm --filter devbackup build",
    "install:cli": "chmod +x script/bin/devbackup && ln -sf $(pwd)/script/bin/devbackup /usr/local/bin/devbackup",
    "test:cli": "script/bin/devbackup",
    "publish:cli": "pnpm --filter devbackup publish"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
